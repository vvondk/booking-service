<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.booking.dao.ProductPriceDao">
	<select id="selectProductPrice" parameterType="Integer" resultType="com.booking.dto.ProductPriceDto">
		SELECT * 
		FROM product_price
		WHERE product_id = #{productId}
	</select>	
</mapper>